{
    "name": "rmcgirr83/zodiacs",
    "type": "phpbb-extension",
    "description": "This Extension will allow a zodiac to appear if the user has inputted month and day in the birthday settings. Birthdays must be allowed on the forum for the extension to work.",
    "version": "1.0.6",
    "time": "2024-07-17",
    "keywords": [
        "phpbb",
        "extension",
        "zodiacs",
        "communication"
    ],
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Rich McGirr",
            "role": "Developer"
        }
    ],
 	"require": {
        "php": ">=7.4",
        "phpbb/phpbb": ">=3.3",
        "composer/installers": "~1.0"
	},
    "extra": {
        "display-name": "Zodiacs",
        "soft-require": {
            "phpbb/phpbb": ">=3.3"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/zodiacs",
            "filename": "version_check"
        }
    }
}